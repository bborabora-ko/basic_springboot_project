{{>layouts/header}}

<table class="table">
    <thead>
    <tr>
        <th scope="col">ID</th>
        <th scope="col">Title</th>
        <th scope="col">Content</th>
    </tr>
    </thead>
    <tbody>
        {{#article}} <!-- 모델에 있는 데이터는 #을 통해 가져올 수 있음-->
        <tr>
            <th>{{id}}</th>
            <td>{{title}}</td>
            <td>{{content}}</td>
        </tr>
        {{/article}} <!-- 가져온 데이터를 여기까지만 쓸거임 -->
    </tbody>
</table>

<a href="/articles">Go to Article List</a>

{{>layouts/footer}}
